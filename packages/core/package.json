{
	"name": "@kitledger/core",
	"version": "0.0.7",
	"private": false,
	"license": "Apache-2.0",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/main.d.ts",
			"default": "./dist/main.js"
		},
		"./db": {
			"types": "./dist/db.d.ts",
			"default": "./dist/db.js"
		},
		"./entities": {
			"types": "./dist/entities.d.ts",
			"default": "./dist/entities.js"
		},
		"./fields": {
			"types": "./dist/fields.d.ts",
			"default": "./dist/fields.js"
		},
		"./transactions": {
			"types": "./dist/transactions.d.ts",
			"default": "./dist/transactions.js"
		},
		"./ui": {
			"types": "./dist/ui.d.ts",
			"default": "./dist/ui.js"
		},
		"./units": {
			"types": "./dist/units.d.ts",
			"default": "./dist/units.js"
		}
	},
	"files": [
		"!dist/**/*.test.js",
		"dist"
	],
	"scripts": {
		"build": "tsc && copyfiles -u 1 \"src/migrations/**/*\" dist",
		"typecheck": "tsc --noEmit"
	},
	"dependencies": {
		"@kitledger/query": "workspace:*",
		"@node-rs/argon2": "^2.0.2",
		"drizzle-orm": "^0.45.1",
		"jose": "^6.1.3",
		"knex": "^3.1.0",
		"pg": "^8.16.3",
		"postgres": "^3.4.7",
		"uuid": "^13.0.0",
		"valibot": "^1.1.0"
	},
	"devDependencies": {
		"@faker-js/faker": "^10.1.0",
		"copyfiles": "^2.4.1",
		"drizzle-kit": "^0.31.8"
	},
	"publishConfig": {
		"access": "public"
	}
}
